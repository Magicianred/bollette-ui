ation();\r\n\r\n    props.closeModal();\r\n  };\r\n  return (\r\n    <div className=\"modal\" onClick={closeModal} style={divStyle}>\r\n      <div className=\"modal-content\" onClick={(e) => e.stopPropagation()}>\r\n        <h5 className=\"close\" onClick={closeModal}>\r\n          &times;\r\n        </h5>\r\n        <h2>{props.title}</h2>\r\n        <div className=\"modal_content\">\r\n          {!props.error && (\r\n            <>\r\n              {props.maxwin && (\r\n                <p>\r\n                  Max win <span>{props.maxwin}</span>$\r\n                </p>\r\n              )}\r\n              {props.ticketId && (\r\n                <p>\r\n                  Ticket id <span>{props.ticketId}</span>\r\n                </p>\r\n              )}\r\n              {props.account_sum && (\r\n                <p>\r\n                  Your balance <span>{props.account_sum}</span>$\r\n                </p>\r\n              )}\r\n            </>\r\n          )}\r\n          {props.error && (\r\n            <>\r\n              <h4 className=\"modal_error_status\">{props.status}</h4>\r\n              <p>{props.error}</p>\r\n            </>\r\n          )}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\nexport default Modal;\r\n","C:\\Users\\Mirko\\Documents\\bollette\\ui\\src\\components\\Footer.js",[],"C:\\Users\\Mirko\\Documents\\bollette\\ui\\src\\screens\\AboutMeComponent.js",[],"C:\\Users\\Mirko\\Documents\\bollette-ui\\src\\index.js",[],["179","180"],"C:\\Users\\Mirko\\Documents\\bollette-ui\\src\\App.js",[],"C:\\Users\\Mirko\\Documents\\bollette-ui\\src\\store.js",[],"C:\\Users\\Mirko\\Documents\\bollette-ui\\src\\screens\\LogInComponent.js",["181"],"import React, { useState, useEffect } from \"react\";\r\nimport { useHistory } from \"react-router\";\r\nimport { useSelector } from \"react-redux\";\r\nimport { login } from \"../service\";\r\nimport { Link } from \"react-router-dom\";\r\nimport { GetUser } from \"../redux/actions\";\r\n\r\nexport const LogInComponent = () => {\r\n  const [usernameOrEmail, setUsernameOrEmail] = useState(\"\");\r\n  const [password, setPassword] = useState(\"\");\r\n  const [focused, setFocused] = useState();\r\n\r\n  const [errors, setErrors] = useState([]);\r\n  const user = useSelector((state) => state.userReducer);\r\n  const history = useHistory();\r\n  useEffect(() => {\r\n    if (user.id) history.replace(\"/\");\r\n  }, [user]);\r\n  const handleSubmit = async () => {\r\n    try {\r\n      const res = await login(usernameOrEmail, password);\r\n      localStorage.setItem(\"token\", res.headers[\"x-auth-token\"]);\r\n      GetUser(res.data);\r\n    } catch (error) {\r\n      setErrors(error.response.data.split(\"-\"));\r\n    }\r\n  };\r\n  return (\r\n    <section className=\"login_component\">\r\n      <h1 className=\"login_title\">Log in</h1>\r\n      <div className=\"login_component_container\">\r\n        <label\r\n          htmlFor=\"emailOrUsername\"\r\n          className={focused === \"emailOrUsername\" ? \"label\" : \"hidden\"}\r\n        >\r\n          Email or Username\r\n        </label>\r\n        <input\r\n          id=\"emailOrUsername\"\r\n          onFocus={() => {\r\n            setFocused(\"emailOrUsername\");\r\n            setErrors();\r\n          }}\r\n          onChange={(e) => setUsernameOrEmail(e.target.value)}\r\n          placeholder=\"username or email\"\r\n        />\r\n        <label\r\n          htmlFor=\"password\"\r\n          className={focused === \"password\" ? \"label\" : \"hidden\"}\r\n        >\r\n          Password\r\n        </label>\r\n\r\n        <input\r\n          id=\"password\"\r\n          className=\"password\"\r\n          type=\"password\"\r\n          onFocus={() => {\r\n            setFocused(\"password\");\r\n            setErrors();\r\n          }}\r\n          onChange={(e) => setPassword(e.target.value)}\r\n          placeholder=\"password\"\r\n        />\r\n        <div className=\"login_buttons\">\r\n          <